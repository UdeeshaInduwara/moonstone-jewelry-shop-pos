  <target name="compile.module.jewelryshop" depends="compile.module.jewelryshop.production,compile.module.jewelryshop.tests" description="Compile module JewelryShop"/>
  
  <target name="compile.module.jewelryshop.production" depends="register.custom.compilers" description="Compile module JewelryShop; production classes">
    <mkdir dir="${jewelryshop.output.dir}"/>
    <javac2 destdir="${jewelryshop.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.jewelryshop}/javac">
      <compilerarg line="${compiler.args.jewelryshop}"/>
      <bootclasspath refid="jewelryshop.module.bootclasspath"/>
      <classpath refid="jewelryshop.module.production.classpath"/>
      <src refid="jewelryshop.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.jewelryshop"/>
    </javac2>
    
    <copy todir="${jewelryshop.output.dir}">
      <fileset dir="${module.jewelryshop.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.jewelryshop.tests" depends="register.custom.compilers,compile.module.jewelryshop.production" description="compile module JewelryShop; test classes" unless="skip.tests"/>
  
  <target name="clean.module.jewelryshop" description="cleanup module">
    <delete dir="${jewelryshop.output.dir}"/>
    <delete dir="${jewelryshop.testoutput.dir}"/>
